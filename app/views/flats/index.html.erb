<p style="color: green"><%= notice %></p>

<h1>Flats</h1>

<div id="flats">
  <% @flats.each do |flat| %>
    <%= render flat %>
    <p>
      <%= link_to "Show this flat", flat %>
    </p>
  <% end %>
</div>
<%= @markers %>
<%= link_to "New flat", new_flat_path %>
<div id='map' style='width: 900px; height: 600px;'
data-controller="map"
data-map-markers-value="<%=@markers%>"
></div>
